{
  "name": "matsumi",
  "version": "1.0.0",
  "description": "A CLI tool for initializing repositories with npm or pnpm",
  "main": "dist/cli.js",
  "module": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "matsumi": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/cli.ts --dts --format cjs,esm --out-dir dist",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts"
  },
  "keywords": [
    "cli",
    "npm",
    "pnpm",
    "initialize"
  ],
  "author": "Domains18",
  "licenses": [
    {
      "type": "ISC",
      "url": "https://opensource.org/licenses/ISC"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Domains18/matsumi.git"
  },
  "bugs": {
    "url": "https://github.com/Domains18/matsumi/issues"
  },
  "homepage": "https://github/Domains18/matsumi#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^9.5.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "typescript": "^4.9.5"
  }
}
